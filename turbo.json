{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".dist/**",".next/**", "!.next/cache/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "dev-watch": {
      "persistent": true,
      "cache": false
    },
    "dev-watch:examples": {
      "dependsOn": ["xterio-auth#dev-watch"], 
      "outputs": []
    }
  }
}