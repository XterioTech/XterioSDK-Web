{
  "name": "xterio-auth",
  "version": "0.0.1",
  "description": "xterio-auth",
  "author": "Clover",
  "license": "ISC",
  "keywords": [
    "xterio-auth"
  ],
  "main": "./dist/lib/index.js",
  "module": "./dist/es/index.js",
  "typings": "./dist/lib/index.d.ts",
  "types": "./dist/es/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/es/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/lib/index.js"
    },
    "./es": "./dist/es/index.js",
    "./es/*.js": "./dist/es/*.js",
    "./es/*": "./dist/es/*.js",
    "./lib": "./dist/lib/index.js",
    "./lib/*.js": "./dist/lib/*.js",
    "./lib/*": "./dist/lib/*.js",
    "./*": "./*"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/chenwei0922/xterio-sdk.git#README",
  "repository": {
    "type": "git",
    "url": "https://github.com/chenwei0922/xterio-sdk.git",
    "directory": "xterio-auth"
  },
  "bugs": {
    "url": "https://github.com/chenwei0922/xterio-sdk/issues"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "dev": "vite --port 3000",
    "build": "npm run build:build && npm run build:types",
    "build:build": "NODE_ENV=production tsx sh/build.ts",
    "build:types": "tsc -P tsconfig.build.json --declaration --emitDeclarationOnly --outDir dist/types"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.24.7",
    "@rollup/plugin-replace": "^5.0.7",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.3.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "consola": "^3.2.3",
    "esbuild": "^0.23.0",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "sass": "^1.77.8",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.0.3",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "dependencies": {
    "axios": "^1.7.3",
    "query-string": "^9.1.0"
  }
}
