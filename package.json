{
  "name": "xterio-auth",
  "version": "1.0.0",
  "description": "xterio-auth",
  "scripts": {
    "test": "jest",
    "dev": "vite --port 3000",
    "build": "npm run build:build && npm run build:types",
    "build:build": "NODE_ENV=production tsx sh/build.ts",
    "build:types": "tsc -P tsconfig.build.json --declaration --emitDeclarationOnly --outDir dist/types"
  },
  "keywords": [
    "xterio-auth"
  ],
  "author": "Clover",
  "license": "ISC",
  "homepage": "https://github.com/chenwei0922/xterio-sdk.git#README",
  "repository": {
    "type": "git",
    "url": "https://github.com/chenwei0922/xterio-sdk.git",
    "directory": "xterio-auth"
  },
  "bugs": {
    "url": "https://github.com/chenwei0922/xterio-sdk/issues"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./*",
        "./dist/types/*"
      ]
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.24.7",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.3.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "consola": "^3.2.3",
    "esbuild": "^0.23.0",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "dependencies": {
    "axios": "^1.7.3",
    "query-string": "^9.1.0"
  }
}
